                            <TextBlock x:Name="PerfectMovesCounTextBlock"
                                       Margin="0"
                                       VerticalAlignment="Center"
                                       Foreground="#787878"
                                       Style="{StaticResource PhoneTextNormalStyle}" />
                        </StackPanel>
                    </StackPanel>
