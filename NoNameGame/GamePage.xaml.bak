                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*"></RowDefinition>
                        <RowDefinition Height="*"></RowDefinition>
                    </Grid.RowDefinitions>


                    <StackPanel Grid.Row="0" Grid.Column="0" Background="{StaticResource PhoneBackgroundBrush}">
                        <StackPanel       Margin="6,6,0,0"                    
                            HorizontalAlignment="Left"
                            VerticalAlignment="Center"
                            Orientation="Horizontal">
                            <TextBlock x:Name="LevelStaticText"
                                       Margin="0"
                                       VerticalAlignment="Center"
                                       FontSize="30"
                                       Foreground="#787878"
                                       Style="{StaticResource CustomHeadline1Font}"

                                       Text="{Binding LocalizedResources.GamePage_LevelTextBlock,
                                          Source={StaticResource LocalizedStrings}}" />
                            <TextBlock x:Name="LevelTextBoxTextBlock"
                                       Margin="0"
                                       Foreground="#787878"
                                       VerticalAlignment="Center"
                                       FontSize="30"

                                       Style="{StaticResource CustomHeadline1Font}" />
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Margin="6,0,0,6">
                            <TextBlock Name="LevelPackTextBlock" Foreground="#787878" FontWeight="SemiBold" />
                            <TextBlock Text=" - "></TextBlock>
                            <TextBlock Name="LevelGroupTextBlock" Foreground="#787878" FontWeight="SemiBold" />
                        </StackPanel>
                    </StackPanel>
                    <StackPanel Grid.Row="1" VerticalAlignment="Top" Grid.Column="0" Margin="6,3,6,6" Orientation="Horizontal">
                        
