<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--  <Color x:Key="CheckedAreaColor">#E3CF36</Color>  -->
    <!--  <Color x:Key="UnCheckedAreaColor">#365CE3</Color>  -->
    <Color x:Key="UnCheckedAreaColor">#ED3E67</Color>
    <Color x:Key="CheckedAreaColor">#EDA528</Color>
    <Color x:Key="ThirdAreaColor">#437019</Color>

    <SolidColorBrush x:Key="CheckedAreaGradientBrush" Color="{StaticResource CheckedAreaColor}" />
    <SolidColorBrush x:Key="UnCheckedAreaGradientBrush" Color="{StaticResource UnCheckedAreaColor}" />
    <SolidColorBrush x:Key="OverlayGradientBlue" Color="{StaticResource CheckedAreaColor}" />
    <SolidColorBrush x:Key="OverlayGradientRed" Color="{StaticResource UnCheckedAreaColor}" />
    <SolidColorBrush x:Key="OverlayGradientGreen" Color="{StaticResource UnCheckedAreaColor}" />

</ResourceDictionary>